    "start": "ts-node ./src/index.ts"
  },
  "files": [
    "bin/"
  ],
  "dependencies": {
    "@octokit/rest": "^15.17.0",
    "jsonfile": "^5.0.0",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "@types/jsonfile": "^4.0.1",
    "@types/node": "^10.12.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.1.6"
  }
}
